{
  "name": "@typeroute/devtools",
  "version": "0.7.1",
  "license": "MIT",
  "author": "strblr",
  "description": "Devtools for TypeRoute router - inspect routes, matches, and navigation state",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strblr/typeroute.git"
  },
  "homepage": "https://typeroute.com",
  "bugs": "https://github.com/strblr/typeroute/issues",
  "keywords": [
    "typeroute",
    "router",
    "devtools",
    "react",
    "typescript",
    "debug"
  ],
  "scripts": {
    "build": "tsc --noEmit && tsdown",
    "dev": "tsdown --watch",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "@types/react": "^19.2.11",
    "tsdown": "^0.20.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@typeroute/router": ">=0.7.0",
    "react": ">=18"
  }
}
